{
  "name": "MyElectronDemo",
  "version": "1.1.0",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wenlittleoil/electron-demo.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
